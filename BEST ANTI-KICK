local p = game:GetService("Players").LocalPlayer
local k = {}

local oldhmmnc
oldhmmnc = hookmetamethod(game, "__namecall", function(self, ...)
    if self == p and getnamecallmethod():lower() == "kick" then
        return
    end
    return oldhmmnc(self, ...)
end)

local f
f = hookfunction(p.Kick, function(s, ...)
    if s == p then
        return
    end
    return f(s, ...)
end)

local p = game:GetService("Players").LocalPlayer
local r = p.PlayerGui.GuiThatShowsPosition.remote

local O; O = hookfunction(r.FireServer, function(s, ...)
    if s == r then
        local a = {...}
        for _, v in ipairs(a) do
            if type(v) == "string" and string.find(string.lower(v), "kick") then
                return
            end
        end
        return O(s, ...)
    end
    return O(s, ...)
end)

local _n
_n = hookmetamethod(game, "__namecall", function(s, ...)
    if s == p and getnamecallmethod():lower() == "kick" then
        return
    end
    return _n(s, ...)
end)

local K
K = hookfunction(p.Kick, function(self, ...)
    if self == p then
        return
    end
    return K(self, ...)
end)

for k, v in pairs(getgc(true)) do
    if pcall(function()
        return rawget(v, "indexInstance")
    end) and type(rawget(v, "indexInstance")) == "table" and (rawget(v, "indexInstance"))[1] == "kick" then
        setreadonly(v, false)
        v.tvk = {
            "kick",
            function()
                return game.Workspace:WaitForChild("")
            end
        }
    end
end
